.block
  .content
    .inner
      %p
        %b First name:
        = @employee.employee_firstName
      %p
        %b Last name:
        = @employee.employee_lastName
      %p
        %b Branch:
        = @employee.branch_id
      %p
        %b Employee dateemployed:
        = @employee.employee_dateEmployed
      %p
        %b Job:
        = @employee.job_id
      %p
        %b Department:
        = @employee.department_id
      %p
        %b Birthday:
        = @employee.employee_birthday
      %p
        %b Age:
        = @employee.employee_age
      %p
        %b Contact number:
        = @employee.employee_contactNumber
      %p
        %b SSS:
        = @employee.employee_sss
      %p
        %b TIN:
        = @employee.employee_tin
      %p
        %b Address:
        = @employee.employee_address
      = link_to 'Edit', edit_employee_path(@employee)
      |
      \#{link_to 'Back', employees_path}
