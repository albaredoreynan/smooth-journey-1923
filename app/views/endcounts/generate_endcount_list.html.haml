.block
  = render 'shared/menu/inventory'
  .content
    .title_action_bar
      %h2 EndCount Entries
    .inner
      %table
        %tr
          %th Endcount Entry Date
          %th Action
          %th View Directional  
        - @item_count.each do |ic|
          %tr
            %td= ic.entry_date.strftime("%B %d, %Y")
            %td= link_to 'View Item Counts', endcounts_path(:date => ic.entry_date)
            %td= link_to 'View Directional Reports', directional_path()
