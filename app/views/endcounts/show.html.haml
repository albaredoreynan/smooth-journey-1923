.block
  .content
    /%h2.title New purchase
    .inner
      %p
        %b Beginning date:
        = @endcount.begin_date.strftime("%B %d, %Y")
      %p
        %b End date:
        = @endcount.end_date.strftime("%B %d, %Y")
